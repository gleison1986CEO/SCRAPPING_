def Sem_Acoes():
     time.sleep(13)
     ##driver_EAD.get("https://www.myip.com/")
     driver_EAD.implicitly_wait(50)
     driver_EAD.get("https://inscricoes.unicesumar.edu.br/bolsaunicesumar/administracao/?utm_campaign=seguidor")
     time.sleep(40)
 
     
def Com_Acoes():
     time.sleep(13)
     ##driver_EAD.get("https://www.myip.com/")
     driver_EAD.implicitly_wait(50)
     driver_EAD.get("https://inscricoes.unicesumar.edu.br/bolsaunicesumar/administracao/?utm_campaign=seguidor")
     l = driver_EAD.find_element(By.CLASS_NAME, "form-control")
     l.send_keys("Leandro")
     l = driver_EAD.find_element(By.ID, "javisEmail")
     l.send_keys("l@l.com")
     l = driver_EAD.find_element(By.ID, "javisTelefone")
     l.send_keys("2199876530")
     l = driver_EAD.find_element(By.ID, "javisCpf")
     l.send_keys("12345678901")
     teste = driver_EAD.find_element(By.CLASS_NAME, "select2")
     teste.click()
     teste2 = driver_EAD.find_element(By.CLASS_NAME, "select2-search__field")
     CURSOS__ = ['Agronegócio','Administração','Análise e Desenvolvimento de Sistemas','Arquitetura e Urbanismo','Artes Visuais','Automação Industrial','Biomedicina','Ciências Biológicas','Educação Física - Licenciatura','Design de Interiores','Teologia','Logística'] 
     teste2.send_keys(random.choice(CURSOS__))
     teste2.send_keys(Keys.ENTER)
     time.sleep(40)
    
    
    
    
    
def Com_requests():
      time.sleep(13)
      driver_EAD.implicitly_wait(50)
      requests.get("http://ead.unicesumar.edu.br/yFbdOq", proxies=proxies_lista_request)
      time.sleep(113)
     


     
     
     
     
     
     
     
         
try:
    
    
    for Analise_01 in range(14):
     time.sleep(14)
     Sem_Acoes()
     print(Analise_01, 'SEM AÇÔES')
     time.sleep(32)
     Com_Acoes()
     print(Analise_01, 'http://ead.unicesumar.edu.br/yFbdOq')
     
 
    for Analise_02  in range(18):
     time.sleep(14)
     Sem_Acoes()
     print(Analise_01, 'SEM AÇÔES')
     time.sleep(16)
     Com_Acoes()
     print(Analise_02, 'http://ead.unicesumar.edu.br/yFbdOq')


finally:
    ##FINALIZAR AÇỗES::::::::::::::::::::::::::::::::::::::::::::::::::::::
    ##FINALIZAR AÇỗES::::::::::::::::::::::::::::::::::::::::::::::::::::::
    ##FINALIZAR AÇỗES::::::::::::::::::::::::::::::::::::::::::::::::::::::
